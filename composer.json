{
    "name": "juststeveking/laravel-toolkit",
    "description": "A simple toolkit package to help me do what I do with Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "role": "Developer",
            "name": "Steve McDougall",
            "email": "juststevemcd@gmail.com",
            "homepage": "https://www.juststeveking.uk/"
        }
    ],
    "autoload": {
        "psr-4": {
            "JustSteveKing\\LaravelToolkit\\": "src/"
        }
    },
    "require": {
        "php": "^8.0|^8.1"
    },
    "require-dev": {
        "laravel/laravel": "^8.6"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
